<?xml version='1.0' encoding='UTF-8' ?>
<portlet-app xmlns='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' version='2.0'>
	
	<portlet>
		<description>AgreuhGator</description>
		<portlet-name>agreugator1</portlet-name>
		<display-name>AgreuhGator View</display-name>
		<portlet-class>fr.giwi.portlet.agreuhgator.view.AgreuhGator</portlet-class>
		
		<expiration-cache>0</expiration-cache>
		
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<resource-bundle>resources.Language</resource-bundle>
		
		<portlet-info>
			<title>AgreuhGator</title>
			<short-title>AgreuhGator</short-title>
		</portlet-info>

		
	</portlet>
	
	
	<portlet>
		<description>AgreuhGator Admin</description>
		<portlet-name>agreugator2</portlet-name>
		<display-name>AgreuhGator</display-name>
		<portlet-class>fr.giwi.portlet.agreuhgator.admin.Admin</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<resource-bundle>resources.Language</resource-bundle>
		<portlet-info>
			<title>AgreuhGator - Admin</title>
			<short-title>AgreuhGatorAdm</short-title>
		</portlet-info>
	</portlet>
</portlet-app>
