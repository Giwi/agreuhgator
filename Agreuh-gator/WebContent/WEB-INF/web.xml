<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<context-param>
		<param-name>pbeans.stores</param-name>
		<param-value>mystore</param-value>
	</context-param>
	<context-param>
		<param-name>mystore.jdbc.url</param-name>
		<!-- Note the &amp; -->
		<param-value>jdbc:mysql://localhost/agreugator?user=root&amp;password=</param-value>
  </context-param>
 <context-param>
    <param-name>mystore.jdbc.driver</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>mystore.max.connections</param-name>
    <param-value>10</param-value>
  </context-param>
  <context-param>
    <param-name>mystore.connection.timeout</param-name>
    <param-value>3600000</param-value>
  </context-param>



  <context-param>
    <param-name>title</param-name>
    <param-value>Agreuh Gator</param-value>
  </context-param>
  <display-name>Agreuh-gator</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>fr.giwi.agreugator.listener.ContextListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>Search</display-name>
    <servlet-name>Search</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.Search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddRss</display-name>
