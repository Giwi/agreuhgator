  <servlet-mapping>
    <servlet-name>AddRss</servlet-name>
    <url-pattern>/addRss</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Rss</display-name>
    <servlet-name>Rss</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.Rss</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Rss</servlet-name>
    <url-pattern>/rss</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Login</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.Login</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>EditNews</display-name>
    <servlet-name>EditNews</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.EditNews</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditNews</servlet-name>
    <url-pattern>/editNews</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EditRss</display-name>
    <servlet-name>EditRss</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.EditRss</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditRss</servlet-name>
    <url-pattern>/editRss</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddNews</display-name>
    <servlet-name>AddNews</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.AddNews</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddNews</servlet-name>
    <url-pattern>/addNews</url-pattern>
  </servlet-mapping>
  <servlet>
