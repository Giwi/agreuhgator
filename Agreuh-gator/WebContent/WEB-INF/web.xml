  <servlet-mapping>
    <servlet-name>DelNews</servlet-name>
    <url-pattern>/delNews</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DelRss</display-name>
    <servlet-name>DelRss</servlet-name>
    <servlet-class>fr.giwi.agreugator.servlet.DelRss</servlet-class>
  </servlet>
  <servlet-mapping>
