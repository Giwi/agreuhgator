    <servlet-class>fr.giwi.agreugator.servlet.BlogRss</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BlogRss</servlet-name>
    <url-pattern>/blogRss</url-pattern>
  </servlet-mapping>
</web-app>